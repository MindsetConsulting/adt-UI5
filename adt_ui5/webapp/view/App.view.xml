<mvc:View
    controllerName="mindset.adt.ui5.adtui5.controller.App"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    id="App"
    >
    <Table id="_IDGenTable1" items="{/Employees}">
        <columns>
            <Column id="_IDGenColumn1">
                <Text id="_IDGenText1" text="Name"/>
            </Column>
            <Column id="_IDGenColumn2">
                <Text id="_IDGenText2" text="Department"/>
            </Column>
            <Column id="_IDGenColumn3">
                <Text id="_IDGenText3" text="Start Date"/>
            </Column>
            <Column id="_IDGenColumn4">
                <Text id="_IDGenText4" text="Role"/>
            </Column>
        </columns>
        <items>
            <ColumnListItem id="_IDGenColumnListItem1">
                <cells>
                    <Text id="_IDGenText5" text="{Name}"/>
                    <Text id="_IDGenText6" text="{Department}"/>
                    <Text id="_IDGenText7" text="{StartDate}"/>
                    <Text id="_IDGenText8" text="{Role}"/>
                </cells>
            </ColumnListItem>
        </items>
    </Table>
</mvc:View>


